<?xml version="1.0"?>

<odoo>
    <data>
        <template id="ing_contrato_locacion_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <div class="full-a4-vert-height">
                                <div class="row" style="border-bottom: 1px solid black;">

                                    <div class="row text-center" style="margin-top: 80px;">
                                        <div class="col-md-12">
                                            <span t-raw="str(o.reemplazar_contenido(o))"/>
                                        </div>
                                    </div>

                                </div>

                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>